<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>
<!-- <?import javafx.scene.control.TextArea?> -->

<VBox stylesheets="@secondary.css" alignment="CENTER" spacing="20.0"
    xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="graph.app.SecondaryController">
    <children>
        <HBox>
            <children>
                <Label fx:id="frameworkNamePreLabel" />
                <Label fx:id="frameworkNameLabel" />
            </children>
        </HBox>
        <HBox>
            <children>
                <Label fx:id="graphNamePreLabel" />
                <Label fx:id="graphNameLabel" />
            </children>
        </HBox>
        <HBox>
            <children>
                <Label fx:id="nodesPreLabel" />
                <Label fx:id="nodesLabel" />
            </children>
        </HBox>
        <HBox>
            <children>
                <Label fx:id="edgesPreLabel"  />
                <Label fx:id="edgesLabel" />
            </children>
        </HBox>
        <HBox>
            <children>
                <Label  fx:id="algorithmPreLabel"/>
                <Label fx:id="algorithmLabel" />
            </children>
        </HBox>
        <HBox>
            <children>
                <Label  fx:id="iterationPreLabel"/>
                <Label fx:id="iterationLabel" />
            </children>
        </HBox>
        <Button fx:id="deploySolutionButton" text="Deploy Solution" onAction="#handleDeploySolution" />
        <Text fx:id="commandText" />
        <!-- <TextArea fx:id="commandTextArea" /> -->
    </children>
</VBox>